{
  "name": "grmvoid/basalt-domain",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Dominik Szamburski",
      "email": "dominikszamburski99@gmail.com"
    }
  ],
  "homepage": "https://github.com/grmvoid/basalt-domain",
  "require": {
    "php": "~8.3.0 || ~8.4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "dev-master",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-strict-rules": "^2.0",
    "phpunit/phpunit": "^12.3.x-dev",
    "vimeo/psalm": "^7.0@dev"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Basalt\\Domain\\": "src/Domain/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BasaltTests\\Domain\\Unit\\": "tests/"
    }
  }
}
